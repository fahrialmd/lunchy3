{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug npm run dev",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/node_modules/.bin/next",
			"args": ["dev"],
			"console": "integratedTerminal",
			"env": {
				"NODE_ENV": "development"
			},
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
